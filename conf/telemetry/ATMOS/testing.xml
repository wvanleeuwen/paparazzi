<?xml version="1.0"?>
<!DOCTYPE telemetry SYSTEM "../telemetry.dtd">
<telemetry>


  <process name="Main">

    <mode name="default">
      <message name="DL_VALUE"          	period="1.2"/>
      <message name="ROTORCRAFT_STATUS"       	period="1.2"/>
      <message name="ROTORCRAFT_FP"          	period="0.8"/>
      <message name="ALIVE"             	period="2.1"/>
      <message name="INS_REF"     		period="5"/>
      <message name="ROTORCRAFT_NAV_STATUS"  	period="1.6"/>
      <message name="WP_MOVED"          	period="1.5"/>
      <message name="GPS_INT"         		period="1.5"/>
      <message name="INS"          		period="1.5"/>
      <!--<message name="TOYTRONICS_SETPOINT" period=".25"/>
      <message name="TOYTRONICS_COMMANDS" period=".25"/>
      <message name="ROTORCRAFT_AHRS_QUAT"   period=".25"/>	-->
      <message name="AHRS_REF_QUAT" 		period="0.8"/>
<!--      <message name="ROTORCRAFT_AHRS_EULER"  period=".1"/>-->
      
      <!--<message name="ROTORCRAFT_AHRS_QUAT"        period=".4"/>
      <message name="IMU_GYRO_SCALED"        period=".2"/>
      <message name="IMU_ACCEL_SCALED"       period=".2"/>
      <message name="IMU_MAG_SCALED"         period=".2"/>
      <message name="FILTER_ALIGNER"         period=".5"/>
      <message name="FILTER"                 period=".5"/>-->

      <message name="HOVER_LOOP"        	period="0.8"/>
      <!--<message name="STAB_ATTITUDE"     period=".4"/>-->
      <message name="HFF_DBG"           	period="1.4"/>
      <!--<message name="STAB_ATTITUDE_REF" period=".4"/>-->
      <message name="HFF_GPS"           	period="1.3"/>
<!--      <message name="SEND_POWER_INFO"           	period="2"/>-->
      <message name="ROTORCRAFT_TUNE_HOVER"        	period="1.2"/>
    </mode>

<!--   <mode name="hispeed_quat_att">
      <message name="DL_VALUE"          	period="6"/>
      <message name="ROTORCRAFT_STATUS"       	period="5"/>
      <message name="ROTORCRAFT_FP"          	period="6"/>
      <message name="ALIVE"             	period="2"/>
      <message name="AHRS_REF_QUAT" 		period="0.2"/>
   </mode>-->

    <mode name="ahrs_debug">
      <message name="ROTORCRAFT_FP"          period=".2"/>
      <message name="AHRS_QUAT_INT"        period=".1"/>
<!--      <message name="IMU_ACCEL_RAW"          period=".1"/>
      <message name="IMU_GYRO_RAW"           period=".1"/>
      <message name="IMU_MAG_RAW"      	     period=".1"/>-->
      <message name="IMU_GYRO_SCALED"        period=".05"/>
      <message name="IMU_ACCEL_SCALED"       period=".05"/>
      <message name="IMU_MAG_SCALED"         period=".2"/>
      <message name="FILTER_ALIGNER"          period="1."/>
      <message name="FILTER"                 period=".1"/>
      <message name="ALIVE"             	period="2.1"/>
    </mode>

    <mode name="ppm">
      <message name="ALIVE"             	period="2.1"/>
      <message name="ROTORCRAFT_CMD"           period=".1"/>
      <message name="PPM"                 period="0.5"/>
      <message name="RC"                  period="0.5"/>
      <message name="ROTORCRAFT_RADIO_CONTROL" period="0.5"/>
      <message name="ROTORCRAFT_STATUS"       period="1"/>
      <message name="DL_VALUE"                period="0.5"/>
      <message name="VFF"               period=".1"/>
      <message name="VERT_LOOP"         period=".1"/>
    </mode>

    <mode name="raw_sensors">
      <message name="ROTORCRAFT_STATUS"       period="1.2"/>
      <message name="DL_VALUE"          period="0.5"/>
      <message name="ALIVE"             period="2.1"/>
      <message name="IMU_ACCEL_RAW"     period=".05"/>
      <message name="IMU_GYRO_RAW"      period=".05"/>
      <message name="IMU_MAG_RAW"       period=".05"/>
      <message name="BARO_RAW"          period=".1"/>
      <message name="ALIVE"             	period="2.1"/>
    </mode>

    <mode name="scaled_sensors">
      <message name="ALIVE"             	period="2.1"/>
      <message name="ROTORCRAFT_STATUS"       period="1.2"/>
      <message name="DL_VALUE"          period="0.5"/>
      <message name="ALIVE"             period="2.1"/>
      <message name="IMU_GYRO_SCALED"        period=".075"/>
      <message name="IMU_ACCEL_SCALED"       period=".075"/>
      <message name="IMU_MAG_SCALED"         period=".1"/>
    </mode>

    <mode name="ahrs">
      <message name="ROTORCRAFT_STATUS"       period="1.2"/>
      <message name="DL_VALUE"          period="0.5"/>
      <message name="ALIVE"             period="2.1"/>
<!--      <message name="ROTORCRAFT_ALIGNER"     period=".1"/> -->
      <message name="FILTER"      period=".5"/>
      <message name="IMU_GYRO_SCALED"        period=".075"/>
      <message name="IMU_GYRO_RAW"      period=".05"/>
      <message name="AHRS_QUAT_INT"   period=".25"/>
<!--      <message name="ROTORCRAFT_AHRS_EULER"  period=".1"/>-->
<!--      <message name="ROTORCRAFT_AHRS_RMAT"   period=".5"/> -->
    </mode>

    <mode name="rate_loop">
      <message name="ROTORCRAFT_STATUS"       period="1.2"/>
      <message name="DL_VALUE"          period="0.5"/>
      <message name="ALIVE"             period="2.1"/>
      <message name="RATE_LOOP"   period=".02"/>
    </mode>

    <mode name="attitude_loop">
      <message name="ROTORCRAFT_STATUS"             period="2"/>
      <message name="DL_VALUE"                period="5"/>
      <message name="ALIVE"                   period="5"/>
      <message name="STAB_ATTITUDE"     period=".2"/>
      <!-- <message name="STAB_ATTITUDE_REF" period=".09"/> -->
      <!--<message name="AHRS_REF_QUAT" period="0.05"/>-->
      <!-- <message name="TOYTRONICS_SETPOINT" period="0.09"/> -->
<!--      <message name="ROTORCRAFT_RADIO_CONTROL" period="0.05"/>
      <message name="BARO_RAW"          period=".2"/>
      <message name="IMU_GYRO_SCALED"        period=".075"/>
      <message name="IMU_ACCEL_SCALED"       period=".075"/>-->
    </mode>

    <mode name="vert_loop">
      <message name="ROTORCRAFT_STATUS"             period="1.2"/>
      <message name="DL_VALUE"                period="0.5"/>
      <message name="ALIVE"                   period="0.9"/>
      <message name="VFF"               period=".05"/>
      <message name="VERT_LOOP"         period=".05"/>
<!--      <message name="ROTORCRAFT_CMD"               period=".05"/> -->
      <message name="INS"               period=".05"/>
      <message name="INS_REF"           period="5.1"/>
    </mode>

    <mode name="h_loop">
      <message name="ALIVE"                   period="0.9"/>
      <message name="HOVER_LOOP"        period="0.062"/>
      <message name="STAB_ATTITUDE"     period=".4"/>
      <message name="HFF_DBG"           period=".2"/>
      <!--<message name="STAB_ATTITUDE_REF" period=".4"/>-->
      <message name="ROTORCRAFT_FP"                period="0.8"/>
      <message name="ROTORCRAFT_STATUS"             period="1.2"/>
      <message name="ROTORCRAFT_NAV_STATUS"        period="1.6"/>
	  <message name="HFF_GPS"           period=".03"/>
      <message name="INS_REF"           period="5.1"/>
    </mode>

    <mode name="aligner">
      <message name="ALIVE"                   period="0.9"/>
      <message name="FILTER_ALIGNER"    period="0.02"/>
    </mode>

    <mode name="hs_att_roll">
      <message name="ROTORCRAFT_STATUS"                 period="1.2"/>
      <message name="ALIVE"                       period="0.9"/>
      <message name="DL_VALUE"                    period="0.5"/>
<!--      <message name="STAB_ATTITUDE_HS_ROLL" period="0.02"/> -->
    </mode>

    <mode name="tune_hover">
      <message name="DL_VALUE"          period="1.1"/>
      <message name="ROTORCRAFT_STATUS"       period="1.2"/>
      <message name="ALIVE"             period="2.1"/>
      <!--<message name="ROTORCRAFT_SONAR"       period="0.1"/>-->
      <!--<message name="ROTORCRAFT_TUNE_HOVER"               period=".1"/>-->
      <!-- <message name="ROTORCRAFT_GPS"               period=".20"/> -->
      <!--<message name="INS2"              period=".05"/>
      <message name="INS3"              period=".20"/>-->
      <message name="INS_REF"           period="5.1"/>
    </mode>


  </process>

</telemetry>
